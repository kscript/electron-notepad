{
  "name": "electron-notepad",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "./src/main.js",
  "scripts": {
    "rollup:main": "rollup -c -i src/main.ts -o src/main.js",
    "rollup:renderer": "rollup -c -i src/renderer.ts -o public/bundle.js -w",
    "start": "npm run rollup:main && electron ."
  },
